{
  "name": "voting-app",
  "version": "0.4.0",
  "description": "make polls, share the link, vote",
  "main": "index.js",
  "author": "Maxim Karabashev",
  "license": "MIT",
  "scripts": {
    "lint": "eslint server src",
    "lint:fix": "npm run lint -- --fix",
    "test": "cross-env NODE_ENV=test karma start config/karma.config.js",
    "test:server": "gulp watch",
    "test:prod": "gulp test:server && cross-env NODE_ENV=test karma start --single-run --no-auto-watch --browsers Chrome,Firefox config/karma.config.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "build": "cross-env NODE_ENV=production webpack --colors --config ./config/webpack.config.prod",
    "build:dev": "cross-env NODE_ENV=development webpack --colors --config ./config/webpack.config.dev-server",
    "deploy": "npm run test:prod && npm run build",
    "start": "forever -c \"node -r dotenv/config\" dist/server/server.js"
  },
