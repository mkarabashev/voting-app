{
  "extends": [
    "../.eslintrc",
    "airbnb"
  ],
  "parser": 'babel-eslint',
  "env": {
    "browser": true
  },
  "plugins": [
    "babel",
    "react",
    "import",
    "jsx-a11y"
  ],
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "./config/webpack.config.prod.js"
      }
    }
  },
  "rules": {
    "import/extensions": 0,
    "linebreak-style": 0,
    "global-require": 0
  }
}
